{
    "View":
    {
        "Renderer":
        {
            "Service": "View.HTML",
            "Method": "Render"
        }
    },
    "HTTP":
    {
        "Force SSL": false,
        "IP":
        {
            "Service": "System.Interface.HTTP.IP",
            "Method": "Get"
        },
        "User Agent":
        {
            "Service": "System.Interface.HTTP.User Agent",
            "Method": "Get"
        },
        "Methods":
        {
            "Allowed": ["GET", "POST"],
            "Default": "GET"
        },
        "Headers":
        {
            "HTTP/1.1": "200 OK",
            "Content-Language:": "ru",
            "X-Frame-Options:": "SAMEORIGIN",
            "X-Content-Type-Options:": "nosniff",
            "X-XSS-Protection:": " 1; mode=block"
        }
    },
    "Locale":
    {
        "Service": "Locale",
        "Method": "Detect"
    },
    "Hooks":
    {
        "beforeInterfaceRun":
        {
            "Output Buffering":
            {
                "Service": "System.Interface.HTTP.OB",
                "Method": "Start"
            }
        },
        "beforeRequestRun":
        {
            "-Journal":
            {
                "Service": "System.Interface.HTTP.Journal"
            },
            "Filter by User Agent":
            {
                "Service": "Security.User Agent",
                "Method": "Filter"
            },
            "Filter Request":
            {
                "Service": "Security.Request",
                "Method": "Filter"
            },
            "IDS":
            {
                "Service": "Security.IDS",
                "Method": "Check"
            },
            "Project":
            {
                "Service": "Project.Preload",
                "Method": "Do"
            }
        },
        "afterInterfaceRun":
        {
            "FastCGI Finish Request":
            {
                "Service": "System.Interface.HTTP",
                "Method": "Finish"
            },
            "IO Shutdown":
            {
                "Service": "IO",
                "Method": "Shutdown"
            },
            "Performance":
            {
                "Service": "Performance",
                "Method": "Do"
            },
            "Spit log":
            {
                "Service": "IO.Log",
                "Method": "Spit"
            },
            "Output Buffering":
            {
                "Service": "System.Interface.HTTP.OB",
                "Method": "Finish"
            }
        }
    }
}