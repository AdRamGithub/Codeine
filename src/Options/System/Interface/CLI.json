{
    "Renderer":
    {
        "Service": "View.Plaintext",
        "Method": "Render"
    },
    "IP": "127.0.0.1",
    "UA": "Shell",
    "Language":
    {
        "Service": "System.Interface.CLI.Language",
        "Method": "Get"
    },
    "Interface":
    {
        "Output":
        {
            "Service": "IO.Print",
            "Method": "Write"
        }
    },
    "Hooks":
    {
        "beforeInterfaceRun":
        {
            "Project":
            {
                "Service": "Project.Preload",
                "Method": "Do"
            },
            "Output Buffering":
            {
                "Service": "System.Interface.Web.OB",
                "Method": "Start"
            }
        },
        "afterInterfaceRun":
        {
            "Rendering":
            {
                "Service":"View",
                "Method":"Render"
            },
            "Output Buffering":
            {
                "Service": "System.Interface.Web.OB",
                "Method": "Finish"
            },
            "Spit log":
            {
                "Service": "IO.Log",
                "Method": "Spit"
            }
        }
    }
}