{
    "View":
    {
        "Renderer":
        {
            "Service": "View.HTML",
            "Method": "Render"
        }
    },
    "Force SSL": false,
    "HTTP":
    {
        "Methods":
        {
            "Allowed": ["GET", "POST"],
            "Default": "GET"
        },
        "Headers":
        {
            "HTTP/1.0": "200 OK",
            "Content-Language:": "ru",
            "X-Frame-Options:": "SAMEORIGIN",
            "X-Content-Type-Options:": "nosniff",
            "X-XSS-Protection:": " 1; mode=block"
        }
    },
    "IP":
    {
        "Service": "System.Interface.Web.IP",
        "Method": "Get"
    },
    "UA":
    {
        "Service": "System.Interface.Web.UA",
        "Method": "Get"
    },
    "Locale":
    {
        "Service": "Locale",
        "Method": "Detect"
    },
    "Subdomains":
    {
        "iframe":
        {
            "Context": "iframe",
            "Headers":
            {
                "X-Frame-Options:": null
            }
        }
    },
    "Hooks":
    {
        "beforeInterfaceRun":
        {
            "Filter by UA":
            {
                "Service": "Security.UA",
                "Method": "Filter"
            },
            "Filter Request":
            {
                "Service": "Security.Request",
                "Method": "Filter"
            },
            "Project":
            {
                "Service": "Project.Preload",
                "Method": "Do"
            },
            "Output Buffering":
            {
                "Service": "System.Interface.Web.OB",
                "Method": "Start"
            }
        },
        "afterInterfaceRun":
        {
            "FastCGI Finish Request":
            {
                "Service": "System.Interface.Web",
                "Method": "Finish"
            },
            "IO Shutdown":
            {
                "Service": "IO",
                "Method": "Shutdown"
            },
            "Performance":
            {
                "Service": "Performance",
                "Method": "Do"
            },
            "Spit log":
            {
                "Service": "IO.Log",
                "Method": "Spit"
            },
            "Output Buffering":
            {
                "Service": "System.Interface.Web.OB",
                "Method": "Finish"
            }
        }
    }
}