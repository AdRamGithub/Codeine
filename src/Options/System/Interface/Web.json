{
    "Interface":
    {
        "Output":
        {
            "Service": "System.Interface.Web",
            "Method": "Output"
        }
    },
    "Headers":
    {
        "HTTP/1.0": "200 OK",
        "Content-type:": "text/html; charset=utf-8",
        "Content-Language:": "ru",
        "Server:": "HAL 9000 / Codeine7",
        "Pragma:": "no-cache",
        "Expires:": "Thu, 1 Jan 1970 03:00:00 GMT"
    },
    "Language":
    {
        "Map":
        {
            "ru": "ru"
        },
        "Default": "ru"
    },
    "Hooks":
    {
        "beforeInterfaceRun":
        {
            "Output Buffering":
            {
                "Service": "System.Interface.Web.OB",
                "Method": "Start"
            }
        },
        "afterInterfaceRun":
        {
            "Spit log":
            {
                "Service": "IO.Log",
                "Method": "Spit"
            },
            "Output Buffering":
            {
                "Service": "System.Interface.Web.OB",
                "Method": "Finish"
            }
        }
    }
}