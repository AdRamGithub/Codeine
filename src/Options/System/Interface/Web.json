{
    "Interface":
    {
        "Output":
        {
            "Service": "System.Interface.Web",
            "Method": "Output"
        }
    },
    "Headers":
    {
        "HTTP/1.0": "200 OK",
        "Content-type:": "text/html; charset=utf-8",
        "Content-Language:": "ru",
        "Cache-Control:": "public",
        "X-Frame-Options:": "DENY",
        "X-Content-Type-Options:": "nosniff",
        "X-XSS-Protection:": " 1; mode=block"
    },
    "Language":
    {
        "Map":
        {
            "ru": "ru"
        },
        "Default": "ru"
    },
    "Hooks":
    {
        "beforeInterfaceRun":
        {
            "Output Buffering":
            {
                "Service": "System.Interface.Web.OB",
                "Method": "Start"
            }
        },
        "afterInterfaceRun":
        {
            "Spit log":
            {
                "Service": "IO.Log",
                "Method": "Spit"
            },
            "Output Buffering":
            {
                "Service": "System.Interface.Web.OB",
                "Method": "Finish"
            }
        }
    }
}