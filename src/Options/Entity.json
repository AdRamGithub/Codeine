{
    "Hooks":
    {
        "afterLoad":
        {
            "Add mixins":
            {
                "Service":"Entity.Model.Mixin",
                "Method":"Process"
            }
        },
        "beforeCreate":
        {
            "Add default values for undefined":
            {
                "Service":"Entity.Nodes.Default",
                "Method":"Process"
            },
            "Datatypes":
            {
                "Service":"Entity.Nodes.Type",
                "Method":"Write"
            },
            "Make variables alive":
            {
                "Service":"Entity.Nodes.Live",
                "Method":"Process"
            },
            "Filter":
            {
                "Service":"Entity.Nodes.Filter",
                "Method":"Process"
            }
        },
        "beforeUpdate":
        {
            "Datatypes":
            {
                "Service":"Entity.Nodes.Type",
                "Method":"Write"
            },
            "Make variables alive":
            {
                "Service":"Entity.Nodes.Live",
                "Method":"Process"
            },
            "Add default values for undefined":
            {
                "Service":"Entity.Nodes.Default",
                "Method":"Process"
            },
            "Filter":
            {
                "Service":"Entity.Nodes.Filter",
                "Method":"Process"
            }
        },
        "afterRead":
        {
            "Datatypes":
            {
                "Service":"Entity.Nodes.Type",
                "Method":"Read"
            }
        }
    }
}