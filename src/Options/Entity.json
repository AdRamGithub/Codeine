{
    "Hooks":
    {
        "beforeOperation":
        {
            "Substitute":
            {
                "Service": "Entity.Hooks.Substitute",
                "Method": "beforeOperation"
            }
        },
        "beforeEntityWrite":
        {
            "Load model (essential)":
            {
                "Service":"Entity",
                "Method":"Load"
            },
            "Strict":
            {
                "Service":"Entity.Nodes.Strict",
                "Method":"Process"
            },
            "Typecast":
            {
                "Service":"Entity.Nodes.Typecast",
                "Method":"Process"
            },
            "Datatypes":
            {
                "Service":"Entity.Nodes.Type",
                "Method":"Write",
                "Call":
                {
                    "Purpose": "Create"
                }
            },
            "Make variables alive":
            {
                "Service":"Entity.Nodes.Hook",
                "Method":"Process"
            },
            "Validate":
            {
                "Service":"Entity.Validate",
                "Method":"Process"
            }
        },
        "beforeEntityCreate":
        {
            "Make variables alive":
            {
                "Service":"Entity.Nodes.Hook",
                "Method":"Process"
            },
            "Default values":
            {
                "Service":"Entity.Nodes.Default",
                "Method":"Write"
            },
            "Filter":
            {
                "Service":"Entity.Nodes.Filter",
                "Method":"Process",
                "Call":
                {
                    "Purpose": "Create"
                }
            }
        },
        "beforeEntityUpdate":
        {
            "Filter":
            {
                "Service":"Entity.Nodes.Filter",
                "Method":"Process"
            },
            "Updated values":
            {
                "Service":"Entity.Nodes.Updated",
                "Method":"Process"
            }
        },
        "beforeEntityDelete":
        {
            "Load model (essential)":
            {
                "Service":"Entity",
                "Method":"Load"
            },
            "Typecast":
            {
                "Service":"Entity.Nodes.Typecast",
                "Method":"Process"
            }
        },
        "beforeEntityRead":
        {
            "Load model (essential)":
            {
                "Service":"Entity",
                "Method":"Load"
            },
            "Typecast":
            {
                "Service":"Entity.Nodes.Typecast",
                "Method":"Process"
            }
        },
        "afterEntityWrite":
        {
            "Make variables alive":
            {
                "Service":"Entity.Nodes.Hook",
                "Method":"Process"
            }
        },

        "afterEntityRead":
        {
            "Default values":
            {
                "Service":"Entity.Nodes.Default",
                "Method":"Read"
            },
            "Datatypes":
            {
                "Service":"Entity.Nodes.Type",
                "Method":"Read"
            },
            "Make variables alive":
            {
                "Service":"Entity.Nodes.Hook",
                "Method":"Process",
                "Call":
                {
                    "Purpose": "Read"
                }
            }
        }
    }
}