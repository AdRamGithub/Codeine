{
    "Regex":
    {
        "Last Searches":
        {
            "Match":"^/searchqueries/last$",
            "Service":"Entity.List",
            "Method":"Do",
            "Call":
            {
                "Entity": "Search.Query",
                "Template": "Last",
                "Sort":
                {
                    "Modified": false
                }
            }
        },
        "Top Searches":
        {
            "Match":"^/searchqueries/top$",
            "Service":"Entity.List",
            "Method":"Do",
            "Call":
            {
                "Entity": "Search.Query",
                "Template": "Top",
                "Sort":
                {
                    "Count": false
                }
            }
        }
    }
}