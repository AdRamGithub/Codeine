{
    "Routers": ["Rewrite", "Static", "Regex"],
    "Hooks":
    {
        "beforeRoute":
        {
            "-Extension":
            {
                "Service": "Code.Routing.Hooks.Extension",
                "Method": "beforeRoute"
            }
        },
        "afterRoute":
        {
            "-Extension":
            {
                "Service": "Code.Routing.Hooks.Extension",
                "Method": "afterRoute"
            }
        }
    }
}