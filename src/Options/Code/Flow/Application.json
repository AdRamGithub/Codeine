{
    "Hooks":{
        "beforeRun":
        {
            "Audit":
            {
                "Service": "Security.Auth",
                "Method": "Audit"
            },
            "Access":
            {
                "Service": "Code.Flow.Front.Access",
                "Method": "Check"
            }
        },
        "afterRun":{
            "Rendering":
            {
                "Service":"View",
                "Method":"RenderSlice"
            }
        },
        "on404":{
            "ErrorBlock":{
                "Service":"Error.404",
                "Method":"Block"
            }
        },
        "Access":
        {
            "Denied":
            {
                "ErrorBlock":
                {
                    "Service": "Error.403",
                    "Method": "Block"
                }
            }
        }
    }
}