{
    "Contract":
    {
        "Code.Flow.Application":
        {
            "Run":
            {
                "Memo": ["Run.Service", "Run.Method", "Run.Call", "Time", "Session.ID"]
            }
        }
    },
    "View":
    {
        "Renderer":
        {
            "Service": "View.HTML",
            "Method": "Render"
        }
    },
    "Context": "app",
    "Run":
    {
        "Method": "Do"
    },
    "Hooks":
    {
        "beforeApplicationRun":
        {
            "Layouts":
            {
                "Service": "Code.Flow.Application.Layouts",
                "Method": "Do"
            },
            "Prepare":
            {
                "Service": "Code.Flow.Front.Before",
                "Method": "Do"
            },
            "Access":
            {
                "Service": "Code.Flow.Application.Access",
                "Method": "Check"
            }
        },
        "afterApplicationRun":
        {
            "Rendering":
            {
                "Service":"View.HTML",
                "Method":"Pipeline"
            }
        },
        "NotFound":
        {
            "Error":
            {
                "Service":"Error.404",
                "Method":"Block"
            }
        },
        "Access":
        {
            "Denied":
            {
                "ErrorBlock":
                {
                    "Service": "Error.403",
                    "Method": "Block"
                }
            }
        }
    }
}