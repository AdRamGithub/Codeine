{
    "MaxChilds":32,
    "RT": 5,
    "PID":
    {
        "Name": "codeined",
        "Prefix": "/var/run/",
        "Postfix": ".pid"
    },
    "Daemons":
    {
        "Tic Tac":
        {
            "Precision": 1,
            "Execute":
            {
                "Service": "System.Time",
                "Method": "Get"
            }
        },
        "Delayed":
        {
            "Precision": 1,
            "Execute":
            {
                "Service": "Code.Run.Delayed",
                "Method": "Execute"
            }
        }
    },
    "Signals":
    {
        "SIGTERM":
        {
            "Service":"Code.Flow.Daemon",
            "Method":"Stop"
        },
        "SIGINT":
        {
            "Service":"Code.Flow.Daemon",
            "Method":"Stop"
        },
        "SIGQUIT":
        {
            "Service":"Code.Flow.Daemon",
            "Method":"Stop"
        },
        "SIGHUP":
        {
            "Service":"Code.Flow.Daemon",
            "Method":"Flush"
        }
    },
    "Codes":
        [
            "0",
            "SIGHUP",
            "SIGINT",
            "SIGQUIT",
            "SIGILL",
            "SIGTRAP",
            "SIGABRT",
            "SIGBUS",
            "SIGFPE",
            "SIGKILL",
            "SIGUSR1",
            "SIGSEGV",
            "SIGUSR2",
            "SIGPIPE",
            "SIGALRM",
            "SIGTERM",
            "SIGSTKFLT",
            "SIGCHLD",
            "SIGCONT",
            "SIGSTOP",
            "SIGTSTP",
            "SIGTTIN",
            "SIGTTOU",
            "SIGURG",
            "SIGXCPU",
            "SIGXFSZ",
            "SIGVTALRM",
            "SIGPROF",
            "SIGIO"
        ]
}