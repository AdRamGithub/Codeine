{
    "Hooks":
    {
        "beforeRun":
        {
            "Routing":
            {
                "Service": "Code.Flow.Front.Routing",
                "Method": "Route"
            },
            "Audit":
            {
                "Service": "Security.Auth",
                "Method": "Audit"
            },
            "Access":
            {
                "Service": "Code.Flow.Front.Access",
                "Method": "Check"
            }
        },
        "afterRun":
        {
            "Rendering":
            {
                "Service":"View",
                "Method":"Render"
            }
        },
        "on404":
        {
            "ErrorPage":
            {
                "Service": "Error.404",
                "Method": "Page"
            }
        },
        "Access":
        {
            "Denied":
            {
                "ErrorPage":
                {
                    "Service": "Error.403",
                    "Method": "Page"
                }
            }
        }
    }
}