{
    "Hooks":
    {
        "beforeRun":
        {
            "Routing":
            {
                "Service": "Code.Flow.Front.Routing",
                "Method": "Route"
            }
        },
        "afterRun":
        {
            "Rendering":
            {
                "Service":"Engine.View",
                "Method":"Render"
            }
        },
        "on404":
        {
            "ErrorPage":
            {
                "Service": "Error.404",
                "Method": "Page"
            }
        }
    }
}