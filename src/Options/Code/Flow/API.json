{
    "API":
    {
        "Formats":
        {
            "json":
            {
                "View":
                {
                    "Renderer":
                    {
                        "Service": "View.JSON",
                        "Method": "Render"
                    }
                }
            }
        }
    },
    "Hooks":
    {
        "beforeAPIRun":
        {
            "Project":
            {
                "Service": "Project.Preload",
                "Method": "beforeFrontRun"
            },
            "Routing":
            {
                "Service": "Code.Flow.API.Routing",
                "Method": "beforeAPIRun"
            },
            "APIKey":
            {
                "Service": "Code.Flow.API.APIKey",
                "Method": "beforeAPIRun"
            },
            "Access":
            {
                "Service": "Code.Flow.API.Access",
                "Method": "beforeAPIRun"
            },
            "QoS":
            {
                "Service": "System.QoS",
                "Method": "Start"
            }
        },
        "afterAPIRun":
        {
            "Rendering":
            {
                "Service":"View",
                "Method":"Render"
            },
            "Latency Audit":
            {
                "Service": "Performance.Latency",
                "Method": "Audit"
            }
        }
    }
}