{
    "Mixins": ["User.Static","Control.Static"],
    "Links":
    {
        "/about/project":
        {
            "Service": "Project.About",
            "Method": "Do"
        },
        "/":
        {
            "Service":"Project.Frontpage",
            "Method":"Do"
        },
        "/check":
        {
            "Service": "User.Validate",
            "Method": "Do",
            "Call":
            {
                "Key":"EMail"
            }
        },
        "/translate":
        {
            "Service": "Locale.Add",
            "Method": "Do",
            "Renderer":
            {
                "Service": "View.JSON",
                "Method": "Render"
            }
        },
        "/sitemap.xml":
        {
            "Service": "SEO.Sitemap",
            "Method": "Do",
            "Renderer":
            {
                "Service": "View.XML",
                "Method": "Render"
            }
        },
        "/monitor.json":
        {
            "Service": "Monitor",
            "Method": "Do",
            "Renderer":
            {
                "Service": "View.JSON",
                "Method": "Render"
            }
        },
        "/uploader":
        {
            "Service": "IO.Uploader",
            "Method": "Do",
            "Renderer":
            {
                "Service": "View.JSON",
                "Method": "Render"
            }
        },
        "/robots.txt":
        {
            "Service": "SEO.Robots",
            "Method": "Do",
            "Renderer":
            {
                "Service": "View.Plaintext",
                "Method": "Render"
            }
        }
    }
}
