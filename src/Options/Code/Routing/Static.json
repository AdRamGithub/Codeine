{
    "Links":
    {
        "/":
        {
            "Service":"Project.Frontpage",
            "Method":"Do"
        },
        "/control":
        {
            "Service": "Control.Panel",
            "Method": "Do",
            "Zone": "Control"
        },
        "/join":
        {
            "Service":"Entity.Create",
            "Method":"Do",
            "Call":
            {
                "Entity": "User"
            }
        },
        "/login":
        {
            "Service":"User.Authenticate",
            "Method":"Do",
            "Call":
            {
                "Mode": "Password"
            }
        },
        "/logout":
        {
            "Service":"User.Annulate",
            "Method":"Do"
        },
        "/forgot":
        {
            "Service":"User.Reset",
            "Method":"Do"
        },
        "/changepwd":
        {
            "Service":"User.Passwd",
            "Method":"Do"
        },
        "/auth/social":
        {
            "Service":"User.Authenticate",
            "Method":"Do",
            "Call":
            {
                "Mode": "Loginza"
            }
        },
        "/check":
        {
            "Service": "User.Validate",
            "Method": "Do",
            "Call":
            {
                "Key":"EMail"
            }
        },
        "/translate":
        {
            "Service": "Locale.Add",
            "Method": "Do"
        }
    }
}
