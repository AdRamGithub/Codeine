{
    "Mixins": ["User.Static","Control.Static"],
    "Links":
    {
        "/about/project":
        {
            "Service": "Project.About",
            "Method": "Do"
        },
        "/payment":
        {
            "Service": "Payment.PaysIO",
            "Method": "Do"
        },
        "/":
        {
            "Service":"Project.Frontpage",
            "Method":"Do"
        },
        "/search":
        {
            "Service": "Entity.Search",
            "Method": "Do"
        },
        "/translate":
        {
            "Service": "Locale.Add",
            "Method": "Do",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.JSON",
                    "Method": "Render"
                }
            }
        },
        "/sitemap.xml":
        {
            "Service": "SEO.Sitemap",
            "Method": "Do",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.XML",
                    "Method": "Render"
                }
            }
        },
        "/uploader":
        {
            "Service": "IO.Uploader",
            "Method": "Do",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.JSON",
                    "Method": "Render"
                }
            }
        },
        "/beacon":
        {
            "Service": "Codeine.Beacon",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.File"
                }
            }
        },
        "/robots.txt":
        {
            "Service": "SEO.Robots",
            "Method": "Do",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.Plaintext",
                    "Method": "Render"
                }
            }
        },
        "/opensearch.xml":
        {
            "Service": "Project.OpenSearch",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.XML",
                    "Method": "Render"
                }
            }
        },
        "/terms":
        {
            "Service": "Project.Terms",
            "Method": "Do"
        }
    }
}
