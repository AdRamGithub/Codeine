{
    "Mixins": ["User.Static","Control.Static"],
    "Links":
    {
        "/about/project":
        {
            "Service": "Project.About",
            "Method": "Do"
        },
        "/oauth":
        {
            "Service": "Services.Social.VK",
            "Method": "Authenticate"
        },
        "/auth/vk":
        {
            "Service": "Services.Social.VK",
            "Method": "Identificate"
        },
        "/":
        {
            "Service":"Project.Frontpage",
            "Method":"Do"
        },
        "/check":
        {
            "Service": "User.Validate",
            "Method": "Do",
            "Call":
            {
                "Key":"EMail"
            }
        },
        "/translate":
        {
            "Service": "Locale.Add",
            "Method": "Do",
            "Renderer":
            {
                "Service": "View.JSON",
                "Method": "Render"
            }
        },
        "/sitemap.xml":
        {
            "Service": "SEO.Sitemap",
            "Method": "Do"
        },
        "/monitor.json":
        {
            "Service": "Monitor",
            "Method": "Do",
            "Renderer": "View.JSON"
        },
        "/uploader":
        {
            "Service": "IO.Uploader",
            "Method": "Do",
            "Renderer": "View.JSON"
        },
        "/robots.txt":
        {
            "Service": "SEO.Robots",
            "Method": "Do",
            "Renderer": "View.Plaintext"
        }
    }
}
