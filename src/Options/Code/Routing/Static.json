{
    "Mixins": ["User.Static","Control.Static"],
    "Links":
    {
        "/about/project":
        {
            "Service": "Project.About",
            "Method": "Do"
        },
        "/test/theme":
        {
            "Service": "Project.Theme",
            "Method": "Do"
        },
        "/payment":
        {
            "Service": "Payment.PaysIO",
            "Method": "Do"
        },
        "/":
        {
            "Service":"Project.Frontpage",
            "Method":"Do"
        },
        "/search":
        {
            "Service": "Search",
            "Method": "Query"
        },
        "/search.rss":
        {
            "Service": "Search",
            "Method": "Query",
            "Call":
            {
                "Link": "/search"
            },
            "View":
            {
                "Renderer":
                {
                    "Service": "View.RSS",
                    "Method": "Render"
                }
            }
        },
        "/search.json":
        {
            "Service": "Search",
            "Method": "Query",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.JSON",
                    "Method": "Render"
                }
            }
        },
        "/translate":
        {
            "Service": "Locale.Add",
            "Method": "Do",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.JSON",
                    "Method": "Render"
                }
            }
        },
        "/sitemap.xml":
        {
            "Service": "SEO.Sitemap",
            "Method": "Index",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.XML",
                    "Method": "Render"
                }
            }
        },
        "/uploader":
        {
            "Service": "IO.Uploader",
            "Method": "Write",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.JSON",
                    "Method": "Render"
                }
            }
        },
        "/beacon":
        {
            "Service": "Codeine.Beacon",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.File"
                }
            }
        },
        "/robots.txt":
        {
            "Service": "SEO.Robots",
            "Method": "Do",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.Plaintext",
                    "Method": "Render"
                }
            }
        },
        "/opensearch.xml":
        {
            "Service": "Project.OpenSearch",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.XML",
                    "Method": "Render"
                }
            }
        },
        "/404":
        {
            "Service": "Error.404",
            "Method": "Page"
        },
        "/403":
        {
            "Service": "Error.403",
            "Method": "Page",
            "Zone": "Default"
        },
        "/terms":
        {
            "Service": "Project.Terms",
            "Method": "Do"
        },
        "/api":
        {
            "Service": "Code.Flow.API",
            "Method": "Run"
        }
    }
}
