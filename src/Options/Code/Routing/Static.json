{
    "Mixins":    [
        "Control.Static",
        "User.Static",
        "Search.Static",
        "Developer.Static",
        "SEO.Static",
        "Project.Static"
    ],
    "Static":
    {
        "/": {
            "Service": "Project.Frontpage",
            "Method":  "Do"
        }
    },
    "/uploader": {
        "Service": "IO.Uploader",
        "Method":  "Write",
        "View":    {
            "Renderer": {
                "Service": "View.JSON",
                "Method":  "Render"
            }
        },
        "/api":    {
            "Service": "Code.Flow.API",
            "Method":  "Do"
        }
    }
}
