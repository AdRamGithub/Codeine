{
    "Mixins": ["User.Static","Control.Static"],
    "Links":
    {
        "/about":
        {
            "Service": "Project.About",
            "Method": "Do"
        },
        "/":
        {
            "Zone": ["Project"],
            "Service":"Project.Frontpage",
            "Method":"Do"
        },
        "/check":
        {
            "Service": "User.Validate",
            "Method": "Do",
            "Call":
            {
                "Key":"EMail"
            }
        },
        "/translate":
        {
            "Service": "Locale.Add",
            "Method": "Do"
        },
        "/sitemap.xml":
        {
            "Service": "SEO.Sitemap",
            "Method": "Do"
        },
        "/monitor.json":
        {
            "Service": "Monitor",
            "Method": "Do",
            "Renderer": "View.JSON"
        },
        "/uploader":
        {
            "Service": "IO.Uploader",
            "Method": "Do",
            "Renderer": "View.JSON"
        }
    }
}
