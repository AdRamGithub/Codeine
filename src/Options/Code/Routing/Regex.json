{
    "Regex":
    {
        "Subcontrol with ID and Action":
        {
            "Match":"@^/control/(.*)/(.*)/(.*)/(.*)$@Ssu",
            "Weight": 45,
            "Service":"Control.Panel",
            "Method":"Do",
            "Zone": ["Control"],
            "Call":
            {
                "Bundle": "$1",
                "Option": "$2",
                "Action": "$3",
                "Where": "$4"
            }
        },
        "Subcontrol with page":
        {
            "Match":"@^/control/(.*)/page(.*)$@Ssu",
            "Weight": 35,
            "Service":"Control.Panel",
            "Method":"Do",
            "Zone": ["Control"],
            "Call":
            {
                "Bundle": "$1",
                "Option": "Do",
                "Page": "$2"
            }
        },
        "Subcontrol with ID":
        {
            "Match":"@^/control/(.*)/(.*)/(.*)$@Ssu",
            "Weight": 25,
            "Service":"Control.Panel",
            "Method":"Do",
            "Zone": ["Control"],
            "Call":
            {
                "Bundle": "$1",
                "Option": "$2",
                "Where": "$3"
            }
        },
        "Subcontrol":
        {
            "Match":"@^/control/(.*)/(.*)$@",
            "Weight": 15,
            "Service":"Control.Panel",
            "Method":"Do",
            "Zone": ["Control"],
            "Call":
            {
                "Bundle": "$1",
                "Option": "$2"
            }
        },
        "Control":
        {
            "Match":"@^/control/(.*)@",
            "Weight": 10,
            "Service":"Control.Panel",
            "Method":"Do",
            "Zone": ["Control"],
            "Call":
            {
                "Bundle": "$1"
            }
        },
        "Activate.User":
        {
            "Match":"@^/activate/user/(.*)@",
            "Service":"User.Activation",
            "Method":"Check",
            "Call":{
                "Code": "$1"
            }
        },
        "SendMail.User":
        {
            "Match": "@^/mail/user/(.*)@",
            "Service":"User.Activation",
            "Method":"Send",
            "Call":{
                "Second": true,
                "Data":
                {
                    "ID": "$1"
                }
            }
        },
        "Auto API":
        {
            "Match": "@^/api/(.*)@",
            "Service":"$1",
            "Method":"Do"
        },
        "Error":
        {
            "Match": "@/error/(.*)@",
            "Service": "Error.Page",
            "Method": "Do",
            "Call":
            {
                "Code": "$1"
            }
        },
        "Auto Doc":
        {
            "Match": "@^/docs/(.*)/(.*)@",
            "Service":"Code.Documentation.ByContract",
            "Method":"Do",
            "Call":
            {
                "Doc":
                {
                    "Service": "$1",
                    "Method": "$2"
                }
            }
        },
        "Experiment":
        {
            "Match": "@^/experiment/(.*)@",
            "Service":"Experiment",
            "Method":"Set",
            "Call":
            {
                "Experiment": "$1"
            }
        },
        "Page":
        {
            "Match": "@/(.*)@",
            "Weight": -5,
            "Service": "Entity.Show.Static",
            "Method": "Do",
            "Zone": ["Project"],
            "Call":
            {
                "Entity": "Page",
                "Where":
                {
                    "Slug": "$1"
                }
            }
        }
    }
}
