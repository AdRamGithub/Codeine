{
    "Regex":
    {
        "Subcontrol":
        {
            "Match":"@^/control/(.*)/(.*)@",
            "Service":"Control.Panel",
            "Method":"Do",
            "Zone": "Control",
            "Call":
            {
                "Bundle": "$1",
                "Option": "$2"
            }
        },
        "Control":
        {
            "Match":"@^/control/(.*)@",
            "Service":"Control.Panel",
            "Method":"Do",
            "Zone": "Control",
            "Call":
            {
                "Bundle": "$1"
            }
        }
    }
}
