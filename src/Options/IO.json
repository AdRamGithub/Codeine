{
    "Storages":
    {
        "Session":
        {
            "Driver": "IO.Storage.Redis",
            "TTL": 0
        },
        "Test":
        {
            "Driver": "IO.Storage.Test"
        },
        "Options":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Options",
            "Format": "Formats.JSON",
            "Suffix":".json"
        },
        "Layout":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets"
        },
        "Locale":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Format": "Formats.JSON",
            "Suffix":".json"
        },
        "CSS":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".css"
        },
        "CSS Cache":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"css",
            "Suffix":".css"
        },
        "LESS":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".less"
        },
        "JS":{
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".js"
        },
        "JS Cache":{
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"js",
            "Suffix":".js"
        },
        "Run Cache":
        {
            "Driver":"IO.Storage.XCache"
        },
        "Run Queue":
        {
            "Driver":"IO.Queue.SPL"
        },
        "Developer":
        {
            "Driver":"IO.Log.ChromePhp"
        },
        "EMail":
        {
            "Driver": "IO.Transport.Mail",
            "Headers":
            {
                "From:": "Codeine"
            }
        },
        "Web":
        {
            "Driver": "IO.Network.HTTP.Curl"
        }
    }
}
