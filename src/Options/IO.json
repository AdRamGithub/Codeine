{
    "Storages":
    {
        "Test":
        {
            "Driver": "IO.Storage.Test"
        },
        "Session":
        {
            "Driver": "IO.Storage.Redis"
        },
        "Static":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Data",
            "Suffix":".html"
        },
        "Layout":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets"
        },
        "Locale":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Format": "Formats.JSON",
            "Suffix":".json"
        },
        "CSS":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".css"
        },
        "CSS Cache":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"css",
            "Suffix":".css"
        },
        "LESS":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".less"
        },
        "JS":{
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".js"
        },
        "JS Cache":{
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"js",
            "Suffix":".js"
        },
        "Run Cache":
        {
            "Driver":"IO.Storage.XCache"
        },
        "Run Queue":
        {
            "Driver":"IO.Queue.SPL"
        },
        "Developer":
        {
            "Driver":"IO.Log.ChromePhp"
        }
    },
    "Methods":
    {
        "Open":
        {
            "Caching": true
        }
    }
}
