{
    "Storages":
    {
        "JSON":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Data",
            "Format": "Formats.JSON",
            "Suffix":".json"
        },
        "Test":
        {
            "Driver": "IO.Test"
        },
        "Options":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Options",
            "Format": "Formats.JSON",
            "Suffix":".json"
        },
        "Layout":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix": ".html"
        },
        "Locale":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Format": "Formats.JSON",
            "Suffix":".json"
        },
        "Image":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".png"
        },
        "Image Cache":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"images",
            "Suffix":".png"
        },
        "CSS":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".css"
        },
        "CSS Cache":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"css",
            "Suffix":".css"
        },
        "LESS":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".less"
        },
        "JS":{
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".js"
        },
        "JS Cache":{
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"js",
            "Suffix":".js"
        },
        "Developer":
        {
            "Driver":"IO.Null"
        },
        "EMail":
        {
            "Driver": "IO.Transport.Mail",
            "Headers":
            {
                "From:": "Codeine"
            }
        },
        "Web":
        {
            "Driver": "IO.Network.HTTP.Curl",
            "Timeout": 15,
            "CookieFile": "cookie.txt",
            "Follow": true,
            "Cache":
            {
                "Driver":"IO.Storage.Directory",
                "Directory":"Data",
                "Scope":"Cache"
            }
        }
    }
}
