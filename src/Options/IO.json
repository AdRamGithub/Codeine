{
    "Storages":
    {
        "Static":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Data",
            "Suffix":".html"
        },
        "Layout":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets"
        },
        "Locale":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Format": "Formats.JSON",
            "Suffix":".json"
        },
        "CSS":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".css"
        },
        "CSS Cache":
        {
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"css",
            "Suffix":".css"
        },
        "JS":{
            "Driver":"IO.Storage.Directory",
            "Directory":"Assets",
            "Suffix":".js"
        },
        "JS Cache":{
            "Driver":"IO.Storage.Directory",
            "Directory":"Public",
            "Scope":"js",
            "Suffix":".js"
        }
    },
    "Methods":
    {
        "Open":
        {
            "Caching": true
        }
    }
}
