{
    "Regex":
    {
        "Message":
        {
            "Match": "/message/(.*)",
            "Service": "Entity.Show.Static",
            "Method": "Do",
            "Call":
            {
                "Entity": "Message",
                "Where": "$1"
            }
        },
        "Delete message":
        {
            "Match": "/message/delete/(.*)",
            "Service": "Entity.Delete",
            "Method": "Do",
            "Call":
            {
                "Entity": "Message",
                "Where": "$1"
            }
        }
    }
}