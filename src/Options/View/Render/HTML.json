{
    "Function":
    {
        "Process":
        {
            "Input":
            {
                "Context":
                {
                    "Default": ".html",
                    "Required": true,
                    "AutoCorrect":
                    {
                        "Required": true
                    }
                }
            }
        }
    },
    "Hooks":
    {
        "beforePipeline":
        {
            "LoadingLayout":
            {
                "Service": "View.Render.HTML.Layout",
                "Method": "Load",
                "Call":
                {
                    "ID": "Main"
                }
            },
            "Meta":
            {
                "Service":"View.Processors.HTML.Meta",
                "Method":"Inject"
            }
        },
        "afterPipeline":
        {
            "CleanPlaces":
            {
                "Service": "View.Render.Pipeline.Places",
                "Method": "Clean"
            }
        }
    }
}