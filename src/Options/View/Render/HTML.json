{
    "Hooks":
    {
        "beforePipeline":
        {
            "LoadingLayout":
            {
                "Service": "View.Processors.HTML.Layout",
                "Method": "Load",
                "Call":
                {
                    "ID": "Main"
                }
            },
            "Meta":
            {
                "Service":"View.Processors.HTML.Meta",
                "Method":"Inject"
            },
            "Front":{
                "Service":"View.Processors.HTML.Front",
                "Method":"Layouts"
            }
        },
        "afterPipeline":
        {
            "CleanPlaces":
            {
                "Service": "View.Render.Pipeline.Places",
                "Method": "Clean"
            }
        }
    }
}
