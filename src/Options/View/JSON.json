{
    "HTTP":
    {
        "Headers":
        {
            "Content-type:": "application/json"
        }
    },
    "Extension": ".json",
    "Hooks":
    {
        "beforeJSONRender":
        {
            "Parslets":
            {
                "Service":"View.JSON.Parslets",
                "Method":"Process"
            },
            "I18N":
            {
                "Service":"View.HTML.Locale",
                "Method":"Process"
            }
        }
    }
}
