{
    "Headers":
    {
        "Content-type:": "application/json"
    },
    "Extension": ".json",
    "Hooks":
    {
        "afterJSONRender":
        {
            "Parslets":
            {
                "Service":"View.JSON.Parslets",
                "Method":"Process"
            },
            "I18N":
            {
                "Service":"View.HTML.LocaleNG",
                "Method":"Process"
            }
        }
    }
}
