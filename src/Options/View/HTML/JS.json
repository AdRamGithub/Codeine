{
    "JS":
    {
        "Proto": "",
        "Pattern": "<js>(.*?)<\/js>",
        "Links": [],
        "Output": [],
        "Host": "",
        "Scope": "",
        "Pathname": "/cache/js/",
        "Type": "application/javascript",
        "Caching": true,
        "Extension": ".js",
        "Remote":
        {
            "TTL": 86400
        },
        "Inline":
        {
            "Prefix": "$(document).ready(function(){",
            "Postfix": "});"
        }
    },
    "Hooks":
    {
        "afterJSWrite":
        {
            "YUI Compressor":
            {
                "Service":"View.HTML.JS.YUICompressor",
                "Method": "Do"
            }
        },
        "beforeJSOutput":
        {
            "Combine":
            {
                "Service":"View.HTML.JS.Combine",
                "Method": "Do"
            }
        },
        "afterJSOutput":
        {
            "Async":
            {
                "Service":"View.HTML.JS.Async",
                "Method": "Do"
            }
        }
    }
}