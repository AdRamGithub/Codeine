{
    "CSS":
    {
        "Proto": "",
        "Scope": "",
        "Pattern": "<css>(.*?)<\/css>",
        "Inline Pattern": "<cssrun>(.*?)<\/cssrun>",
        "Links": [],
        "Output": [],
        "Host": "",
        "Pathname": "/cache/css/",
        "Type": "text/css",
        "Caching": true,
        "Extension": ".css"
    },
    "Hooks":
    {
        "-afterCSSWrite":
        {
            "YUI Compressor":
            {
                "Service":"View.HTML.CSS.YUICompressor",
                "Method": "Do"
            }
        },
        "beforeCSSOutput":
        {
            "Combine":
            {
                "Service":"View.HTML.CSS.Combine",
                "Method": "Do"
            }
        },
        "beforeCSSWrite":
        {
            "Minify":
            {
                "Service":"View.HTML.CSS.Minify",
                "Method": "Do"
            }
        }
    }
}