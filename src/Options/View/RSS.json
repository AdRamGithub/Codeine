{
    "Layout": "",
    "Widget Set": "",
    "Extension": ".rss",
    "HTTP":
    {
        "Headers":
        {
            "Content-type": "application/rss+xml; charset=utf-8"
        }
    },
    "Hooks":
    {
        "afterRSSPipeline":
        {
            "Parslets":
            {
                "Service":"View.HTML.Parslets",
                "Method":"Process"
            },
            "I18N":
            {
                "Service":"View.HTML.Locale",
                "Method":"Process"
            }
        }
    }
}
