{
    "JS":
    {
        "Proto": "",
        "Pattern": "<js>(.*?)<\/js>",
        "Links": [],
        "Output": [],
        "Host": "",
        "Scope": "",
        "Pathname": "/cache/js/",
        "Type": "application/javascript",
        "Caching": true,
        "Extension": ".js",
        "Remote":
        {
            "TTL": 86400
        },
        "Inline":
        {
            "Prefix": "document.addEventListener('DOMContentLoaded',function(){",
            "Postfix": "});"
        }
    },
    "Hooks":
    {
        "beforeJSOutput":
        {
            "Combine":
            {
                "Service":"View.JS.Combine",
                "Method": "Do"
            }
        },
        "beforeJSWrite":
        {
            "-Obfuscator":
            {
                "Service":"View.JS.Obfuscate",
                "Method": "Do"
            },
            "Compressor":
            {
                "Service":"View.JS.Compress",
                "Method": "Do"
            }
        },
        "afterJSOutput":
        {
            "Async":
            {
                "Service":"View.JS.MakeAsync",
                "Method": "Do"
            }
        }
    }
}