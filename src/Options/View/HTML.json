{
    "Context": "",
    "HTTP":
    {
        "Headers":
        {
            "Content-type:": "text/html; charset=utf-8"
        }
    },
    "View":
    {
        "HTML":
        {
            "Widget Set": "bootstrap",
            "Title": [],
            "Keywords": [],
            "Description": []
        }
    },
    "Extension": ".html",
    "Layout": "<place>Content</place>",
    "Place Pattern": "<place>(.*?)<\/place>",
    "Hooks":
    {
        "beforeHTMLPipeline":
        {
            "Layouts":
            {
                "Service":"View.HTML.Layouts",
                "Method":"Do"
            }
        },
        "afterHTMLRender":
        {
            "Call 1":
            {
                "Service": "View.Hooks.Call",
                "Method": "Parse"
            },
            "Parslets":
            {
                "Service":"View.HTML.Parslets",
                "Method":"Process"
            },
            "Images":
            {
                "Service":"View.HTML.Image",
                "Method":"Process"
            },
            "JS":
            {
                "Service":"View.HTML.JS",
                "Method":"Process"
            },
            "CSS":
            {
                "Service":"View.HTML.CSS",
                "Method":"Process"
            },
            "Meta":
            {
                "Service":"View.HTML.Meta",
                "Method":"Process"
            },
            "Breadcrumbs":
            {
                "Service":"View.HTML.Breadcrumbs",
                "Method":"Scan"
            },
            "No Remote":
            {
                "Service":"View.HTML.NoRemote",
                "Method":"Process"
            },
            "Places":
            {
                "Service":"View.HTML.Places",
                "Method":"Clean"
            },
            "Minify":
            {
                "Service":"View.HTML.Minify",
                "Method":"Process"
            },
            "I18N":
            {
                "Service":"View.HTML.Locale",
                "Method":"Process"
            }
        }
    }
}
