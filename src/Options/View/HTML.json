{
    "Headers":
    {
        "Content-type:": "text/html; charset=utf-8"
    },
    "Widget Set": "bootstrap",
    "Extension": ".html",
    "Layout": "<place>Content</place>",
    "Place Pattern": "<place>(.+?)<\/place>",
    "Hooks":
    {
        "beforePipeline":
        {
            "Layouts":
            {
                "Service":"View.HTML.Layouts",
                "Method":"Do"
            }
        },
        "afterPipeline":
        {
            "Parslets":
            {
                "Service":"View.HTML.Parslets",
                "Method":"Process"
            },
            "JS":
            {
                "Service":"View.HTML.JSTag",
                "Method":"Process"
            },
            "CSS":
            {
                "Service":"View.HTML.CSSTag",
                "Method":"Process"
            },
            "Meta":
            {
                "Service":"View.HTML.Meta",
                "Method":"Scan"
            },
            "Breadcrumbs":
            {
                "Service":"View.HTML.Breadcrumbs",
                "Method":"Scan"
            },
            "Places":
            {
                "Service":"View.HTML.Places",
                "Method":"Clean"
            },
            "Minify":
            {
                "Service":"View.HTML.Minify",
                "Method":"Process"
            },
            "I18N":
            {
                "Service":"View.HTML.LocaleNG",
                "Method":"Process"
            }
        }
    }
}
