{
    "Regex":
    {
        "Create service order":
        {
            "Match":"^/order/create/service/(.+)$",
            "Service":"Entity.Create",
            "Method":"Do",
            "Call":
            {
                "Entity": "Company.Order",
                "Data":
                {
                    "Company.CService":
                    {
                        "Service": "Entity",
                        "Method": "Far",
                        "Call":
                        {
                            "Entity": "Company.CService",
                            "Key": "ID",
                            "Where":
                            {
                                "Slug": "$1"
                            }
                        }
                    }
                }
            },
            "Last": true
        },
        "Show my order":
        {
            "Match":"^/my/order/(.+)$",
            "Service":"Entity.Show.Static",
            "Method":"Do",
            "Call":
            {
                "Entity": "Company.Order",
                "Where": "$1"
            }
        },
        "List my orders":
        {
            "Match":"^/my/orders$",
            "Service":"Entity.List",
            "Method":"Do",
            "Call":
            {
                "Entity": "Company.Order",
                "Where":
                {
                    "User": "$Session.User.ID"
                }
            }
        }
    }
}