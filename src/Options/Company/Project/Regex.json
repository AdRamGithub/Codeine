{
    "Regex":
    {
        "Список проектов, страница":
        {
            "Match":"^/projects/page(\\d+)$",
            "Service":"Entity.List",
            "Method":"Do",
            "Call":
            {
                "Entity": "Company.Project",
                "Page":  "$1"
            }
        },
        "Список проектов":
        {
            "Match": "^/projects$",
            "Service": "Entity.List",
            "Method": "Do",
            "Call":
            {
                "Entity": "Company.Project"
            }
        },
        "Список проектов категории, страница":
        {
            "Match":"^/projects/category/(.+)/page(\\d+)$",
            "Service":"Entity.List",
            "Method":"Do",
            "Call":
            {
                "Entity": "Company.Project",
                "Custom Templates":
                {
                    "List": "Category"
                },
                "Where":
                {
                    "Category": "$1"
                },
                "Page":  "$2"
            }
        },
        "Список проектов категории":
        {
            "Match": "^/projects/category/(.+)$",
            "Service": "Entity.List",
            "Method": "Do",
            "Call":
            {
                "Entity": "Company.Project",
                "Custom Templates":
                {
                    "List": "Category"
                },
                "Where":
                {
                    "Category": "$1"
                }
            }
        },
        "Проект":
        {
            "Match":"^/project/(.+)$",
            "Service":"Entity.Show.Static",
            "Method":"Do",
            "Call":
            {
                "Entity": "Company.Project",
                "Where":
                {
                    "Slug": "$1"
                }
            },
            "Last": true
        }
    }
}