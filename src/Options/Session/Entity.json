{
    "Mixins": ["Mixins.Channel"],
    "Storage": "Primary",
    "EPP": 50,
    "Nodes":
    {
        "ID":
        {
            "Type": "Literal.String"
        },
        "URL":
        {
            "Type": "Literal.String"
        },
        "IP":
        {
            "Type": "Numeral.IP",
            "Hooks":
            {
                "beforeEntityWrite":
                {
                    "Service": "System.Interface.Web.IP",
                    "Method": "Get"
                }
            }
        },
        "User":
        {
            "Default": 0,
            "Type": "Numeral.Integer"
        },
        "Secondary":
        {
            "Type": "Numeral.Integer",
            "Default": null,
            "Nullable": true
        },
        "Expire":
        {
            "Type": "Numeral.Integer",
            "Hooks":
            {
                "beforeEntityCreate":
                {
                    "Service": "System.Time",
                    "Method": "Get",
                    "Call":
                    {
                        "Increment": 15552000
                    }
                }
            }
        },
        "Created":
        {
            "Type": "Numeral.Integer",
            "Hooks":
            {
                "beforeEntityCreate":
                {
                    "Service": "System.Time",
                    "Method": "Get"
                }
            }
        },
        "Location":
        {
            "Type": "Numeral.Integer",
            "Default": 0
        }
    },
    "Hooks":
    {
        "afterDeletePost":
        {
           "Redirect":
            {
                "Service": "System.Interface.Web",
                "Method": "Redirect",
                "Call":
                {
                    "Location": "/control/Session"
                }
            }
        }
    }
}