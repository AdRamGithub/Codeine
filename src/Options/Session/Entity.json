{
    "Storage": "Primary",
    "Nodes":
    {
        "ID":
        {
            "Type": "Numeral.Integer",
            "Hooks":
            {
                "beforeEntityCreate":
                {
                    "Service": "Security.UID",
                    "Method": "Get",
                    "Call":
                    {
                        "Mode": "Secure"
                    }
                }
            }
        },
        "Heartbeat":
        {

        },
        "IP.Start":
        {
            "Hooks":
            {
                "beforeEntityCreate":
                {
                    "Service": "System.Interface.Web.IP",
                    "Method": "Get"
                }
            }
        },
        "IP.Current":
        {
            "Hooks":
            {
                "beforeEntityUpdate":
                {
                    "Service": "System.Interface.Web.IP",
                    "Method": "Get"
                }
            }
        },
        "User":
        {
            "Type": "Numeral.Integer"
        },
        "Secondary":
        {
            "Type": "Numeral.Integer",
            "Default": null,
            "Nullable": true
        },
        "Expire":
        {
            "Type": "Numeral.Integer"
        },
        "Created":
        {
            "Hooks":
            {
                "beforeEntityCreate":
                {
                    "Service": "System.Time",
                    "Method": "Get"
                }
            }
        }
    },
    "Hooks":
    {
        "afterDeletePost":
        {
           "Redirect":
            {
                "Service": "System.Interface.Web",
                "Method": "Redirect",
                "Call":
                {
                    "Location": "/control/Session"
                }
            }
        }
    }
}