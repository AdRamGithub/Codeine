{
    "CreateDirectory":
    {
        "_N": "System.FS.Directory",
        "_F": "Create",
        "Value": "/var/www/$Name"
    },
    "EnterDirectory":
    {
        "_N": "System.FS.Directory",
        "_F": "Change",
        "Value": "/var/www/$Name"
    },
    "Git":
    {
        "_N": "System.Linux.Git",
        "_F": "Init"
    },
    "CreateDirectory.Options":
    {
        "_N": "System.FS.Directory",
        "_F": "Create",
        "Value": "/var/www/$Name/Options"
    },
    "CreateDirectory.Drivers":
        {
            "_N": "System.FS.Directory",
            "_F": "Create",
            "Value": "/var/www/$Name/Drivers"
        },
    "CreateDirectory.Data":
        {
            "_N": "System.FS.Directory",
            "_F": "Create",
            "Value": "/var/www/$Name/Data"
        },
    "CreateDirectory.Public":
        {
            "_N": "System.FS.Directory",
            "_F": "Create",
            "Value": "/var/www/$Name/Public"
        },
    "CreateIndex":
    {
        "_N": "System.FS.File",
        "_F": "Copy",
        "From": "$Codeine/index.php",
        "To": "/var/www/$Name/Public/index.php"
    },
    "CreateHost":
    {
        "_N": "System.FS.File",
        "_F": "Create",
        "Filename": "/var/www/$Name/$Name.host",
        "Value":
        {
            "_N": "Utilities.Configuration.Apache.VHost",
            "_F": "Generate",
            "Path": "/var/www/$Name",
            "Host": "$Name"
        }
    },
    "README":
    {
        "_N": "System.FS.File",
        "_F": "Touch",
        "Filename": "$Name/README"
    },
    "CHANGELOG":
        {
            "_N": "System.FS.File",
            "_F": "Touch",
            "Filename": "/var/www/$Name/CHANGELOG"
        },
    "LICENSE":
        {
            "_N": "System.FS.File",
            "_F": "Create",
            "Filename": "/var/www/$Name/LICENSE.txt",
            "Value":
            {
                "_N": "Project.License.$License",
                "_F": "Generate",
                "Year": 2011,
                "Owner": "BreathLess",
                "EMail": "dmitry.bergstein@gmail.com"
            }
        },
    "VERSION":
        {
            "_N": "System.FS.File",
            "_F": "Touch",
            "Filename": "/var/www/$Name/VERSION"
        },
    "All2Git":
        {
            "_N": "System.Linux.Git",
            "_F": "Add.All"
        },
    "Commit":
    {
        "_N": "System.Linux.Git",
        "_F": "Commit.All",
        "Message": "Started Codeine Project"
    }
}