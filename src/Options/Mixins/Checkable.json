{
    "Nodes":
    {
        "Checked":
        {
            "Type": "Numeral.Integer",
            "Hooks":
            {
                "beforeEntityUpdate":
                {
                    "Service": "System.Time",
                    "Method": "Get"
                }
            }
        },
        "Checker":
        {
            "Type": "Numeral.Integer",
            "Hooks":
            {
                "beforeEntityUpdate":
                {
                    "Service": "Session",
                    "Method": "Read",
                    "Call":
                    {
                        "Key": "User.ID"
                    }
                }
            }
        },
        "Check":
        {
            "Type": "Boolean",
            "Default": false
        }
    },
    "Hooks":
    {
        "afterCheck":
        {
            "Restore URL":
            {
                "Service": "Entity.Redirect.Back",
                "Method": "Restore"
            }
        }
    }
}