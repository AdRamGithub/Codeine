{
    "Core Storage Limit": 2048,
    "Verbose":
    {
        "Developer": 4,
        "Administrator": 4,
        "Security": 4,
        "Marketing": 4,
        "Performance": 4
    },
    "Driver":
    {
        "Path": "Drivers",
        "Extension": ".php"
    },
    "Hooks":
    {
        "onBootstrap":
        {
            "Versioning":
            {
                "Service": "Codeine.Versioning",
                "Method": "Do"
            }
        },
        "onShutdown":
        {
            "IO Shutdown":
            {
                "Service": "IO",
                "Method": "Shutdown"
            }
        }
    }
}