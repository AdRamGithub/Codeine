{
    "Facebook":
    {
        "AppID": 0,
        "Secret": "",
        "Rights": "offline_access",
        "Mapping":
        {
            "first_name": "Name",
            "last_name": "Surname",
            "gender": "Gender"
        }
    },
    "Hooks":
    {
        "afterFacebookAuthenticate":
        {
            "Redirect":
            {
                "Service": "System.Interface.HTTP",
                "Method": "Redirect",
                "Call":
                {
                    "Location": "/profile"
                }
            }
        },
        "afterFacebookAnnulate":
        {
            "Redirect":
            {
                "Service": "System.Interface.HTTP",
                "Method": "Redirect",
                "Call":
                {
                    "Location": "/profile"
                }
            }
        }
    }
}