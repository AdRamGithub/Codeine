{
    "Mixins":
    [
        "Control.Security.Access.UserNo1",
        "User.Security",
        "Project.Security",
        "SEO.Security",
        "Developer.Security"
    ],
    "Weight": 0,
    "Decision": false,
    "Rules":
    {
        "ЦУП Роутер доступен всем":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "Control.Only"
            }
        },
        "Главная страница доступна всем":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "Project.Frontpage"
            }
        },
        "Страницы ошибок доступны всем":
        {
            "Decision": true,
            "Weight": 200,
            "Run":
            {
                "Service":"Error.Page"
            }
        },
        "Allow show page to all":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "Entity.Show.Static",
                "Method": "Do",
                "Call":
                {
                    "Entity": "Page"
                }
            }
        },
        "Allow show block to all":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "Entity.Show.Static",
                "Method": "Do",
                "Call":
                {
                    "Entity": "Block"
                }
            }
        },
        "Меню видно всем":
        {
            "Decision": true,
            "Weight": 100,
            "Run":
            {
                "Service": "Entity.List",
                "Method": "Do",
                "Call":
                {
                    "Entity": "Page"
                }
            }
        },
        "Редирект всем":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "System.Interface.HTTP",
                "Method": "Redirect"
            }
        },
        "Удалённый редирект всем":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "System.Interface.HTTP",
                "Method": "Remote Redirect"
            }
        },
        "Жалоба — всем":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "Entity.Complain"
            }
        },
        "404 — всем":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "Error.404"
            }
        },
        "IP Flag — всем":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "Country.FlagByIP"
            }
        },
        "Direct Assets to all":
        {
            "Decision": true,
            "Weight": 50,
            "Run":
            {
                "Service": "View.Asset"
            }
        }
    }
}