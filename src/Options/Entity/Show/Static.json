{
    "Contract":
    {
        "Entity.Show.Static":
        {
            "Do":
            {
                "Memo": ["Entity", "Where", "Template", "Time"]
            }
        }
    },
    "Hooks":
    {
        "beforeShow":
        {
            "Load model (essential)":
            {
                "Service":"Entity",
                "Method":"Load"
            }
        },
        "beforeShowBefore":
        {
            "No Cache Check":
            {
                "Service": "Entity.Hooks.NoCache",
                "Method": "Check"
            }
        },
        "onShowNotFound":
        {
            "Show Error Page":
            {
                "Service": "Error.Page",
                "Method": "Do",
                "Call":
                {
                    "Code": 404
                }
            }
        }
    }
}