{
    "Regex":
    {
        "Уведомления":
        {
            "Match":"^/notifies$",
            "Service":"Entity.List",
            "Method":"Do",
            "Call":
            {
                "Entity": "Notify",
                "Where":
                {
                    "User":
                    {
                        "Service": "Session",
                        "Method": "Read",
                        "Call":
                        {
                            "Key": "User.ID"
                        }
                    }
                },
                "Sort":
                {
                    "Created": false
                }
            }
        },
        "Уведомления / Страницы ":
        {
            "Match":"^/notifies/page([\\d]+)$",
            "Service":"Entity.List",
            "Method":"Do",
            "Call":
            {
                "Entity": "Notify",
                "Page": "$1",
                "Where":
                {
                    "User":
                    {
                        "Service": "Session",
                        "Method": "Read",
                        "Call":
                        {
                            "Key": "User.ID"
                        }
                    }
                },
                "Sort":
                {
                    "Created": false
                }
            }
        },
        "Слушаем":
        {
            "Match":"^/notify/subscribe$",
            "Service":"Notify.Subscribe",
            "Method":"Do",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.JSON",
                    "Method": "Render"
                }
            }
        }
    }
}