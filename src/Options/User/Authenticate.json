{
    "TTLs":
    {
        "Short": 86400,
        "Long": 604800
    },
    "Hooks":
    {
        "afterAuthenticatePost":
        {
            "Redirect":
            {
                "Service": "System.Interface.Web",
                "Method": "Redirect",
                "Call":
                {
                    "Location": "/"
                }
            }
        },
        "Authenticating":
        {
            "Failed":
            {
                "Show Message":
                {
                    "Output":
                    {
                        "Message":
                        {
                            "Incorrect  ":
                            {
                                "Type": "Block",
                                "Class": "alert alert-danger",
                                "Value": "Аутентификация не удалась"
                            }
                        }
                    }
                }
            }
        }
    }
}