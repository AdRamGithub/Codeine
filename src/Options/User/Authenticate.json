{
    "TTLs":
    {
        "Short": 86400,
        "Long": 604800
    },
    "Hooks":
    {
        "beforeAuthenticateGet":
        {
            "Store URL":
            {
                "Service": "System.Interface.Web",
                "Method": "StoreURL"
            }
        },
        "afterAuthenticatePost":
        {
            "Redirect":
            {
                "Service": "System.Interface.Web",
                "Method": "Redirect",
                "Call":
                {
                    "Location": "/"
                }
            }
        }
    }
}