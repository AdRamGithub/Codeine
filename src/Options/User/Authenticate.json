{
    "Hooks":
    {
        "Authenticating":
        {
            "Failed":
            {
                "Fail":
                {
                    "Output":
                    {
                        "Message":
                        {
                            "Error":
                            {
                                "Type": "Block",
                                "Class": "alert alert-danger",
                                "Value": "Аутентификация не удалась"
                            }
                        }
                    }
                }
            },
            "Success":
            {
                "Redirect":
                {
                    "Service": "System.Interface.Web",
                    "Method": "Redirect",
                    "Call":
                    {
                        "Location": "/"
                    }
                }
            }
        }
    }
}