{
    "Links":
    {
        "/join":
        {
            "Service":"Entity.Create",
            "Method":"Do",
            "Call":
            {
                "Entity": "User"
            }
        },
        "/login":
        {
            "Service":"User.Authenticate",
            "Method":"Do",
            "Call":
            {
                "Mode": "Password"
            }
        },
        "/logout":
        {
            "Service":"Session",
            "Method":"Annulate"
        },
        "/forgot":
        {
            "Service":"User.Reset",
            "Method":"Do"
        },
        "/changepwd":
        {
            "Service":"User.Passwd",
            "Method":"Do"
        },
        "/auth/social":
        {
            "Service":"User.Authenticate",
            "Method":"POST",
            "Call":
            {
                "Mode": "Loginza"
            }
        },
        "/cabinet":
        {
            "Service":"User.Cabinet",
            "Method":"Do"
        },
        "/profile":
        {
            "Service":"Entity.Update",
            "Method":"Do",
            "Call":
            {
                "Entity": "User",
                "One": true,
                "Where":
                {
                    "ID":
                    {
                        "Service": "Session",
                        "Method": "Read",
                        "Call":
                        {
                            "Key": "User.ID"
                        }
                    }
                }
            }
        },
        "/user/services":
        {
            "Service":"User.Services",
            "Method":"Do"
        }
    }
}