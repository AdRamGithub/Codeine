{
    "Links":
    {
        "/join":
        {
            "Service":"Entity.Create",
            "Method":"Do",
            "Call":
            {
                "Entity": "User",
                "OnlyRequired": true
            }
        },
        "/login":
        {
            "Service":"User.Authenticate",
            "Method":"Do",
            "Call":
            {
                "Mode": "Password"
            }
        },
        "/logout":
        {
            "Service":"User.Logout",
            "Method":"Do"
        },
        "/forgot":
        {
            "Service":"User.Reset",
            "Method":"Do"
        },
        "/changepwd":
        {
            "Service":"Entity.Update",
            "Call":
            {
                "Purpose": "Password",
                "Only Tag": "Password",
                "Entity": "User",
                "Where":
                {
                    "Service": "Session",
                    "Method": "Read",
                    "Call":
                    {
                        "Key": "User.ID"
                    }
                }
            }
        },
        "/auth/social":
        {
            "Service":"User.Authenticate",
            "Method":"POST",
            "Call":
            {
                "Mode": "Loginza"
            }
        },
        "/cabinet":
        {
            "Service":"User.Cabinet",
            "Method":"Do"
        },
        "/profile":
        {
            "Service":"Entity.Update",
            "Call":
            {
                "Entity": "User",
                "Where": "$Session.User.ID"
            }
        },
        "/user/services":
        {
            "Service":"User.Services",
            "Method":"Do"
        },
        "/takeout":
        {
            "Service":"Entity.Delete",
            "Method":"Do",
            "Call":
            {
                "Entity": "User",
                "Where":
                {
                    "Service": "Session",
                    "Method": "Read",
                    "Call":
                    {
                        "Key": "User.ID"
                    }
                }
            }
        }
    }
}