{
    "Auth Modes": ["Password"],
    "TTLs":
    {
        "Short": 86400,
        "Long": 604800
    },
    "Hooks":
    {
        "beforeLoginDo":
        {
            "Store URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "StoreURL"
            }
        },
        "-afterAuthenticate":
        {
            "Restore URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "RestoreURL"
            }
        },
        "afterAnnulate":
        {
            "Restore URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "RestoreURL"
            }
        }
    }
}